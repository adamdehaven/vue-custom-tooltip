{
  "name": "@adamdehaven/vue-custom-tooltip",
  "version": "2.1.0",
  "description": "A customizable, reusable, and reactive tooltip component for Vue 3 (including TypeScript) projects.",
  "keywords": [
    "Vue",
    "tooltip",
    "popover",
    "component",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adamdehaven/vue-custom-tooltip.git"
  },
  "author": "Adam DeHaven <adam@dehaven.org> (https://www.adamdehaven.com/)",
  "license": "MIT",
  "homepage": "https://github.com/adamdehaven/vue-custom-tooltip/#readme",
  "bugs": {
    "url": "https://github.com/adamdehaven/vue-custom-tooltip/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "del-cli ./dist && rollup -c && del-cli ./dist/main.d.ts",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vue-custom-tooltip.cjs.js",
  "module": "dist/vue-custom-tooltip.esm.js",
  "browser": "dist/vue-custom-tooltip.esm.js",
  "unpkg": "dist/vue-custom-tooltip.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "del-cli": "^3.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.2.1",
    "rollup": "^2.51.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue": "^3.0.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
